{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "lib": [
      "es6",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "isolatedModules": true,
    "jsx": "preserve",
    "resolveJsonModule": true
  },
  "include": [
    "next-env.d.ts",
    "pages/**/*",
    "src/**/*",
    "**/*.ts",
    "**/*.tsx",
    "pages/404.js",
    "components/Navbar/Navbar.js"
, "components/Layouts/HeadForProdcs.tsx", "components/Layouts/HeadForProdcs.tsx", "components/Products/Product/style.js", "components/ShimmerEffect/Shimmer.js", "components/Layouts/layouUserInterface.js", "components/Footer/ModalContact/ContactModal.js", "actions/ProductsAction.js", "actions/LanguageSwitch.js"  ],
  "exclude": [
    "node_modules"
  ]
}
